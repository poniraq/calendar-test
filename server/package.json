{
  "name": "protect",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start": "cross-env NODE_PATH=dist node dist/app.js",
    "start:debug": "cross-env NODE_PATH=dist node --inspect-brk dist/app.js",
    "clean": "rm -rf dist",
    "build": "npm run clean && tsc",
    "db:create": "sequelize db:create",
    "db:drop": "sequelize db:drop",
    "db:recreate": "npm run db:drop && npm run db:create",
    "migrate:up": "sequelize db:migrate",
    "migrate:down": "sequelize db:migrate:undo",
    "migrate:purge": "sequelize db:migrate:undo:all"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@decorators/di": "^1.0.2",
    "@decorators/express": "^2.3.0",
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.3",
    "compose-middleware": "^5.0.0",
    "express": "^4.16.3",
    "express-request-id": "^1.4.0",
    "express-session": "^1.15.6",
    "http-errors": "^1.6.3",
    "jsonwebtoken": "^8.3.0",
    "klaw-sync": "^4.0.0",
    "lodash": "^4.17.10",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^1.0.0",
    "pg": "^7.4.3",
    "pg-hstore": "^2.3.2",
    "reflect-metadata": "^0.1.12",
    "sequelize": "^4.38.0",
    "sequelize-cli": "^4.0.0",
    "sequelize-typescript": "^0.6.5",
    "tslib": "^1.9.3"
  },
  "devDependencies": {
    "@types/express": "^4.16.0",
    "@types/express-session": "^1.15.10",
    "@types/http-errors": "^1.6.1",
    "@types/jsonwebtoken": "^7.2.8",
    "@types/klaw-sync": "^2.0.0",
    "@types/lodash": "^4.14.113",
    "@types/passport": "^0.4.6",
    "@types/pg-promise": "^5.4.3",
    "@types/reflect-metadata": "^0.1.0",
    "@types/sequelize": "^4.27.24",
    "cross-env": "^5.2.0",
    "typescript": "^2.8.3"
  }
}
